.loading-screen {
    position: absolute;
    width: 100%;
    height: 130%;
    left: 0;
    top: -50px;
    align-items: center;
    justify-content: center;
    background-color: #FAE1BE;

}

.loading-background {
    top: -30px;
    width: 350px;
    height: 350px;
    -unity-background-scale-mode: scale-and-crop;
}

.loading-text {
    bottom: 100px;
    font-size: 15px;
    color: #dd9061;
    -unity-font-style: Bold;
}

